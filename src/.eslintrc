{
    "env": {
      "browser": true,
      "es2021": true,
      "jest/globals": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "jest"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
  }
  